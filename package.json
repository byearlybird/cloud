{
	"name": "@byearlybird/monorepo",
	"version": "1.0.0",
	"private": true,
	"workspaces": ["apps/*", "packages/*"],
	"scripts": {
		"dev": "bun run --filter @byearlybird/cloud dev",
		"start": "bun run --filter @byearlybird/cloud start",
		"test": "bun test",
		"build": "bun run --filter @byearlybird/cloud-client build",
		"db:generate": "bun run --filter @byearlybird/cloud db:generate",
		"db:migrate": "bun run --filter @byearlybird/cloud db:migrate",
		"db:push": "bun run --filter @byearlybird/cloud db:push",
		"db:studio": "bun run --filter @byearlybird/cloud db:studio"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.4",
		"typescript": "^5.6.0"
	}
}
